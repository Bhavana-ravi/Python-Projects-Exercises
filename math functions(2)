# Basic Math functions(2)
# Round, Absolute, power, Maximum, Minimum

x = 3.14
y = -4
z = 5

# Round off value
result = round(x)
print(f"Round off value: {result}") 

# Absolute value
result = abs(y)
print(f"Absolute value: {result}") 

# Power
result = pow(z, 5)
print(f"Power: {result}") 

# Maximum value
result = max(x, y, z)
print(f"Maximum value: {result}") 

# Minimum value
result = min(x, y, z)
print(f"Minimum value:{result}") 
